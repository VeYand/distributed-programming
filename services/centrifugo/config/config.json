{
  "client": {
    "token": {
      "hmac_secret_key": "my_secret"
    },
    "allowed_origins": ["*"]
  },
  "http_api": {
    "key": "_salt"
  },
  "admin": {
    "enabled": true
  },
  "namespaces": [
    {
      "name": "statistics",
